{
    "name": "BoecSpecOPs/CakeD",
    "description": "CakeD plugin for CakePHP",
    "type": "cakephp-plugin",
    "require": {
        "dropbox/dropbox-sdk": "1.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "CakeD\\": "src"
        }
    },
    "scripts": {
        "post-update-cmd": "CakeD\\Core::postUpdate",
        "post-install-cmd": "CakeD\\Core::postInstall"
    },
    "autoload": {
        "psr-4": {
            "BoecSpecOPs\\CakeD\\": "./plugins/CakeD/src"
        }
    }
}
